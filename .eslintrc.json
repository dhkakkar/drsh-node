{
  "settings": {
    "import/resolver": {
      "alias": {
        "root": [
          "."
        ],
        "map": [
          [
            "@",
            "./"
          ],
          [
            "@config",
            "./config"
          ],
          [
            "@env",
            "config/env.js"
          ]
        ],
        "extensions": [
          ".js",
          ".json"
        ]
      }
    }
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "parser": "@babel/eslint-parser"
}